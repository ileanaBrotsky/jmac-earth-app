{
  "name": "jmac-earth-backend",
  "version": "1.0.0",
  "description": "Backend para JMAC Earth - Cálculos Hidráulicos de Bombeo",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.cjs --coverage --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.cjs --watch",
    "test:unit": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/unit",
    "test:integration": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/integration",
    "test:e2e": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/e2e",
    "db:migrate": "node src/infrastructure/database/migrations/run.js",
    "db:seed": "node src/infrastructure/database/seeders/run.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix"
  },
  "keywords": [
    "hydraulic",
    "calculations",
    "kmz",
    "gis",
    "clean-architecture"
  ],
  "author": "IBeyond",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@types/jest": "^30.0.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.5",
    "typescript": "^5.9.3"
  }
}