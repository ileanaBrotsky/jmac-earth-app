{
  "name": "jmac-earth-backend",
  "version": "1.0.0",
  "description": "Backend para JMAC Earth - Cálculos Hidráulicos de Bombeo",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "pretest": "bash scripts/check-test-db.sh 2>/dev/null || true",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.cjs --coverage --verbose",
    "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.cjs --watch",
    "test:unit": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/unit",
    "test:integration": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/integration",
    "test:e2e": "cross-env NODE_ENV=test jest --config=jest.config.cjs --testPathPattern=tests/e2e",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -d src/infrastructure/database/data-source.ts",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:run:test": "npx cross-env NODE_ENV=test npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:show": "npm run typeorm -- migration:show",
    "seed:run": "ts-node src/infrastructure/database/seeders/run.ts",
     "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "docker:db": "docker exec -it jmac_earth_postgres psql -U postgres -d jmac_earth_dev",
    "docker:db:test": "docker exec -it jmac_earth_postgres psql -U postgres -d jmac_earth_test",
    "db:setup": "npm run docker:up && sleep 5 && npm run migration:run && npm run seed:run",
    "db:reset": "npm run docker:reset && sleep 5 && npm run migration:run && npm run seed:run",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix"
  },
  "keywords": [
    "hydraulic",
    "calculations",
    "kmz",
    "gis",
    "clean-architecture",
    "typeorm",
    "typescript"
  ],
  "author": "IBeyond",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "jszip": "^3.10.1",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.3",
    "winston": "^3.17.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@eslint/js": "^9.17.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.1",
    "@types/uuid": "^10.0.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
